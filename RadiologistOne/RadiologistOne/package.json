{
  "name": "RadiologistOne",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node ./buildScript",
    "lint": "vue-cli-service lint",
    "install-requirements": "pip install -r server/requirements.txt",
    "start": "concurrently \"yarn start-frontend || npm run start-frontend\" \"yarn start-backend || npm run start-backend\"",
    "start-backend": "python3 server/server.py || py -3 server/server.py || python server/server.py",
    "start-frontend": "vue-cli-service serve --open"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@jupyter-widgets/jupyterlab-manager": "^1.1.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.27",
    "core-js": "^2.6.5",
    "fs-extra": "8.1.0",
    "leapjs": "^0.6.4",
    "requirejs": "^2.3.6",
    "serve": "^11.2.0",
    "vue": "^2.6.6",
    "vue-carousel-3d": "^0.2.0",
    "vue-router": "^3.0.4",
    "vuetify": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "proxy": "http://localhost:3001"
}
